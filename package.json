{
  "name": "webuild-repos",
  "version": "1.0.0",
  "homepage": "https://github.com/webuildorg/webuild-repos",
  "keywords": "repos, repositories, github, open source",
  "main": "index",
  "license": "mit",
  "dependencies": {
    "github": "0.2.4",
    "jsonfile": "2.2.1",
    "mess": "0.1.2",
    "promise": "7.0.4",
    "moment-timezone": "0.4.0",
    "cli-color": "1.0.0",
    "request": "2.60.0"
  },
  "repository" :{
    "type" : "git",
    "url" : "git+https://github.com/webuildorg/webuild-repos.git"
  },
  "config": {
    "blanket": {
      "pattern": [
        "archives/index.js",
        "countdown/index.js",
        "repos/index.js"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
